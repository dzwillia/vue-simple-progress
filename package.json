{
  "name": "vue-simple-progress",
  "version": "1.1.0",
  "description": "A simple, flexible progress bar for Vue.js",
  "author": "David Z. Williams <dave@flex.io>",
  "main": "dist/vue-simple-progress.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --config ./build/webpack.dev.js --open --inline --hot",
    "build:debug": "cross-env NODE_ENV=debug webpack --mode development --config build/webpack.dist.js",
    "build:release": "cross-env NODE_ENV=production webpack --mode production --config build/webpack.dist.js",
    "build:examples": "webpack --mode production --config build/webpack.examples.js",
    "build": "npm run build:debug && npm run build:release && npm run build:examples",
    "clean": "rimraf ./examples/examples.bundle.css ./examples/examples.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dzwillia/vue-simple-progress.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "ui",
    "components",
    "progress",
    "progressbar"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dzwillia/vue-simple-progress/issues"
  },
  "homepage": "https://github.com/dzwillia/vue-simple-progress/",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.4.5",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "deep-assign": "^3.0.0",
    "node-sass": "^4.11.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vue": "^2.5.22",
    "vue-loader": "^15.5.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
